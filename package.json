{
    "name": "vant-environment",
    "version": "3.2.0",
    "description": "Common types and structures used across all vant-packages.",
    "exports": {
        ".": "./dist/index.js",
        "./units": "./dist/units/index.js",
        "./structures": "./dist/structures/index.js",
        "./structures/subtypes": "./dist/structures/subtypes/index.js",
        "./log": "./dist/log/index.js",
        "./utils": "./dist/utils/index.js"
    },
    "typesVersions": {
        "*": {
            "structures": [
                "dist/structures"
            ],
            "structures/subtypes": [
                "dist/structures/subtypes"
            ],
            "units": [
                "dist/units"
            ],
            "log": [
                "dist/log"
            ],
            "utils": [
                "dist/utils"
            ]
        }
    },
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/index.js",
        "docs": "npx typedoc"
    },
    "keywords": [
        "vantjs",
        "vant-environment",
        "davis vantage",
        "pro 2",
        "pro",
        "vue",
        "weather-station"
    ],
    "author": "Henri Hollmann (harrydehix)",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^20.8.2",
        "ts-essentials": "^9.4.0",
        "typedoc": "^0.25.1",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@types/lodash.merge": "^4.6.7",
        "lodash.merge": "^4.6.2",
        "winston": "^3.10.0",
        "winston-daily-rotate-file": "^4.7.1"
    }
}